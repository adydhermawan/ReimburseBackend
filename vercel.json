{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "database",
        "QUEUE_CONNECTION": "sync",
        "MAIL_MAILER": "log"
    }
}