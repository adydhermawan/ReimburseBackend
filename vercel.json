{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://${VERCEL_URL}",
        "ASSET_URL": "https://${VERCEL_URL}",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "database",
        "QUEUE_CONNECTION": "sync",
        "MAIL_MAILER": "log"
    }
}